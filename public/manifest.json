{
  "name": "Push (EPNS) Protocol (Alpha)",
  "description": "Receive Notifications from Web3.0.\n\nIntroducing Push (EPNS) - The Missing Piece of Web3.",
  "version": "0.2.7",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Push (EPNS) Protocol",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'sha256-G3Kz1I8/7XpFKsnzcqwUW6tETCcESP3q0yJbcK+IXDg=' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
